        "egg-validate": "^1.0.0",
        "egg-view-vue-ssr": "^3.0.5",
        "element-ui": "^2.0.8",
        "extend": "~3.0.0",
        "font-awesome": "^4.7.0",
        "lodash": "^4.17.4",
        "lodash-id": "^0.14.0",
        "lowdb": "^1.0.0",
        "mockjs": "^1.0.1-beta3",
